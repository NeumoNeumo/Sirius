PATH = :

[boot]
 DTS_PATH       = /etc/boot/esp32c3.dts

[system_data]
 username       = user
 sysname        = SiriuOS
 HOME           = /etc
 system_data    = /etc/system/data
 system_home    = /etc/system

BOOTLOGO       = %system_data%/bootlogo.dat
SYSTEM_VERSION = %system_data%/system_version.dat

[shell]
shell_home     = %system_home%/console.history              #终端历史存储文件路径


REGISTRY_PATH  = /etc/                                      #系统注册表路径
PYTHON_HOME = /etc/python/lib



